.pingtuna-container {
  width: 100%;
  border-bottom: 20rpx solid #f5f5f5;
}

.pingtuna-title {
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  color: #333;
  font-size: 32rpx;
}

.pingtuna-title image {
  width: 36rpx;
  height: 36rpx;
  margin-bottom: -6rpx;
  margin-left: 10rpx;
}

.pingtuna-container .pingtuna-list {
  display: inline-block;
  width: 49.8%;
  height: 250rpx;
  border-top: 1rpx solid #f5f5f5;
